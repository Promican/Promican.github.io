function sendEvent(t){t=t.closest(".row.jc-sb");t&&gtag("event","odds_click",{deal_type:t.getAttribute("attr-deal"),bookie:t.getAttribute("attr-name"),device:"web",screen_name:t.getAttribute("attr-sub-section"),id:t.getAttribute("attr-id"),type:t.getAttribute("attr-type"),position:t.getAttribute("attr-position"),isocode:document.querySelector("body").getAttribute("data-iso-code")})}function sendOddsEvent(t){t=t.closest(".row-body");t&&gtag("event","odds_click",{deal_type:t.getAttribute("attr-deal"),bookie:t.getAttribute("attr-name"),device:"web",screen_name:t.getAttribute("attr-sub-section"),id:t.getAttribute("attr-id"),type:t.getAttribute("attr-type"),position:t.getAttribute("attr-position"),isocode:document.querySelector("body").getAttribute("data-iso-code")})}document.addEventListener("DOMContentLoaded",function(){var t=document.querySelector(".bet-banner-ls");t&&!t.classList.contains("hidden")&&(t=t.querySelector(".tab-content.active"))&&(t=t.querySelectorAll(".row.jc-sb"))&&t.forEach(t=>{gtag("event","odds_impression",{deal_type:t.getAttribute("attr-deal"),bookie:t.getAttribute("attr-name"),device:"web",screen_name:t.getAttribute("attr-sub-section"),id:t.getAttribute("attr-id"),type:t.getAttribute("attr-type"),position:t.getAttribute("attr-position"),isocode:document.querySelector("body").getAttribute("data-iso-code")})})});