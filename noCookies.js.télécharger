function inmobi(){window.location.href="/payment",document.getElementsByTagName("header")[0].classList.add("bet-blur"),document.getElementsByTagName("footer")[0].classList.add("bet-blur")}countryCode&&"es"===countryCode&&document.addEventListener("DOMContentLoaded",function(){new MutationObserver(e=>{e.forEach(e=>{e.addedNodes.forEach(e=>{"canvas"==e.tagName&&e.remove(),e.classList&&e.classList.contains("qc-cmp2-footer-overlay")&&3===e.childNodes[0].childNodes.length&&e.childNodes[0].childNodes[1].addEventListener("click",inmobi),e.classList&&e.classList.contains("qc-cmp-cleanslate")&&3===e.childNodes[0].childNodes[1].childNodes[0].childNodes.length&&e.childNodes[0].childNodes[1].childNodes[0].childNodes[2].addEventListener("click",inmobi)})})}).observe(document,{childList:!0,subtree:!0})});