const localData=localStorage.getItem("isAdult")??null,localDataMemory=localData?JSON.parse(localData):{name:"no",update:now};function getUKFormat(t,a=11){if(null===t||t<=1)return"-";var o=t-1;let e=0,r=1,n=Number.MAX_VALUE;for(let t=1;t<=a;t++){var l=Math.round(o*t),m=l/t,m=Math.abs(o-m);m<n&&(e=l,r=t,n=m)}return e+"/"+r}function getUSFormat(t){let a=t;return a=2<=t?Math.round(100*(t-1)):Math.round(-100/(t-1)),a=Number.isNaN(a)?"-":a}function getHongKongFormat(t){let a=t-1;return a=a.toFixed(2),a=Number.isNaN(a)?"-":a}function getMalayFormat(t){let a=t;return a=(a=2<=t?-1/(t-1):t-1).toFixed(2),a=Number.isNaN(a)?"-":a}function getIndonesianFormat(t){let a=t;return a=(a=2<=t?t-1:-1/(t-1)).toFixed(2),a=Number.isNaN(a)?"-":a}function convertOdd(t){var a=t.innerText,a=parseFloat(a);let o=a;2==localDataMemory.oddsFormat?o=getUKFormat(a):3==localDataMemory.oddsFormat?o=getUSFormat(a):4==localDataMemory.oddsFormat?o=getHongKongFormat(a):5==localDataMemory.oddsFormat?o=getMalayFormat(a):6==localDataMemory.oddsFormat&&(o=getIndonesianFormat(a)),t.innerText=o}function editOddsFormat(){localDataMemory.oddsFormat,null!==localDataMemory.oddsFormat&&1<localDataMemory.oddsFormat&&(document.getElementsByClassName("bet-banner-ls")&&document.querySelectorAll("span.mh5").forEach(t=>{convertOdd(t)}),document.getElementById("mod_betTabs"))&&document.querySelectorAll(".mh15").forEach(t=>{convertOdd(t)})}